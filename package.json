{
    "name": "toon-formatter",
    "version": "2.1.1",
    "funding": {
        "type": "github",
        "url": "https://github.com/sponsors/ankitpal181"
    },
    "description": "A powerful library to convert between TOON, JSON, YAML, XML, and CSV with end-to-end encryption. Includes Unified Converters for direct format-to-format translation and LLM token optimization.",
    "main": "src/index.js",
    "type": "module",
    "scripts": {
        "test": "node --test test/*.test.js",
        "test:watch": "node --test --watch test/*.test.js"
    },
    "keywords": [
        "toon",
        "json",
        "yaml",
        "xml",
        "csv",
        "converter",
        "llm",
        "token",
        "optimization",
        "data-format",
        "serialization",
        "formatter",
        "encryption",
        "aes-256-gcm",
        "security",
        "crypto",
        "secure-data",
        "end-to-end-encryption",
        "unified-converter",
        "json-to-xml",
        "csv-to-yaml",
        "xml-to-json"
    ],
    "author": "Ankit Pal",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/ankitpal181/toon-formatter-lib"
    },
    "bugs": {
        "url": "https://github.com/ankitpal181/toon-formatter-lib/issues"
    },
    "homepage": "https://github.com/ankitpal181/toon-formatter-lib#readme",
    "engines": {
        "node": ">=16.0.0"
    },
    "dependencies": {
        "js-yaml": "^4.1.0",
        "papaparse": "^5.4.1",
        "xmldom": "^0.6.0"
    },
    "devDependencies": {},
    "exports": {
        ".": "./src/index.js",
        "./json": "./src/json.js",
        "./yaml": "./src/yaml.js",
        "./xml": "./src/xml.js",
        "./csv": "./src/csv.js",
        "./validator": "./src/validator.js",
        "./utils": "./src/utils.js",
        "./encryptor": "./src/encryptor.js",
        "./json-converter": "./src/json_formatter/index.js",
        "./yaml-converter": "./src/yaml_formatter/index.js",
        "./xml-converter": "./src/xml_formatter/index.js",
        "./csv-converter": "./src/csv_formatter/index.js"
    }
}